{
    "dashboard": {
        "id": null,
        "title": "ALYX Collaboration Service Metrics",
        "tags": [
            "alyx",
            "collaboration"
        ],
        "style": "dark",
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "Active Sessions",
                "type": "stat",
                "targets": [
                    {
                        "expr": "alyx_collaboration_sessions_active",
                        "legendFormat": "Active Sessions"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "Active WebSocket Connections",
                "type": "stat",
                "targets": [
                    {
                        "expr": "alyx_collaboration_connections_active",
                        "legendFormat": "Active Connections"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 6,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "Total Participants",
                "type": "stat",
                "targets": [
                    {
                        "expr": "alyx_collaboration_participants_total",
                        "legendFormat": "Total Participants"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "id": 4,
                "title": "Pending Operations",
                "type": "stat",
                "targets": [
                    {
                        "expr": "alyx_collaboration_operations_pending",
                        "legendFormat": "Pending Operations"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": 0
                                },
                                {
                                    "color": "yellow",
                                    "value": 10
                                },
                                {
                                    "color": "red",
                                    "value": 50
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 18,
                    "y": 0
                }
            },
            {
                "id": 5,
                "title": "Message Processing Rate",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(alyx_collaboration_messages_sent_total[5m])",
                        "legendFormat": "Messages Sent/sec"
                    },
                    {
                        "expr": "rate(alyx_collaboration_messages_received_total[5m])",
                        "legendFormat": "Messages Received/sec"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 6,
                "title": "Session Activity",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(alyx_collaboration_sessions_created_total[5m])",
                        "legendFormat": "Sessions Created/sec"
                    },
                    {
                        "expr": "rate(alyx_collaboration_sessions_joined_total[5m])",
                        "legendFormat": "Sessions Joined/sec"
                    },
                    {
                        "expr": "rate(alyx_collaboration_sessions_left_total[5m])",
                        "legendFormat": "Sessions Left/sec"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 8
                }
            },
            {
                "id": 7,
                "title": "Operational Transform Performance",
                "type": "graph",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(alyx_collaboration_operation_transform_duration_bucket[5m]))",
                        "legendFormat": "95th percentile"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(alyx_collaboration_operation_transform_duration_bucket[5m]))",
                        "legendFormat": "50th percentile"
                    }
                ],
                "yAxes": [
                    {
                        "unit": "s"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 16
                }
            },
            {
                "id": 8,
                "title": "Conflict Resolution",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(alyx_collaboration_conflicts_resolved_total[5m])",
                        "legendFormat": "Conflicts Resolved/sec"
                    },
                    {
                        "expr": "histogram_quantile(0.95, rate(alyx_collaboration_conflict_resolution_duration_bucket[5m]))",
                        "legendFormat": "Resolution Time 95th percentile"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 16
                }
            },
            {
                "id": 9,
                "title": "WebSocket Connection Events",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(alyx_collaboration_websocket_connections_total[5m])",
                        "legendFormat": "Connections/sec"
                    },
                    {
                        "expr": "rate(alyx_collaboration_websocket_disconnections_total[5m])",
                        "legendFormat": "Disconnections/sec"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 24,
                    "x": 0,
                    "y": 24
                }
            }
        ],
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "refresh": "5s"
    }
}