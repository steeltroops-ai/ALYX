{
    "dashboard": {
        "id": null,
        "title": "ALYX Data Processing Metrics",
        "tags": [
            "alyx",
            "data-processing"
        ],
        "style": "dark",
        "timezone": "browser",
        "panels": [
            {
                "id": 1,
                "title": "Query Execution Rate",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(alyx_dataprocessing_queries_executed_total[5m])",
                        "legendFormat": "Queries/sec"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 0,
                    "y": 0
                }
            },
            {
                "id": 2,
                "title": "Cache Hit Ratio",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(alyx_dataprocessing_cache_hits_total[5m]) / (rate(alyx_dataprocessing_cache_hits_total[5m]) + rate(alyx_dataprocessing_cache_misses_total[5m])) * 100",
                        "legendFormat": "Hit Ratio %"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "unit": "percent",
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "red",
                                    "value": 0
                                },
                                {
                                    "color": "yellow",
                                    "value": 70
                                },
                                {
                                    "color": "green",
                                    "value": 90
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 6,
                    "y": 0
                }
            },
            {
                "id": 3,
                "title": "Active Database Connections",
                "type": "graph",
                "targets": [
                    {
                        "expr": "alyx_dataprocessing_connections_active",
                        "legendFormat": "Active Connections"
                    },
                    {
                        "expr": "alyx_dataprocessing_connections_idle",
                        "legendFormat": "Idle Connections"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 0
                }
            },
            {
                "id": 4,
                "title": "Query Execution Time",
                "type": "graph",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(alyx_dataprocessing_query_execution_time_bucket[5m]))",
                        "legendFormat": "95th percentile"
                    },
                    {
                        "expr": "histogram_quantile(0.50, rate(alyx_dataprocessing_query_execution_time_bucket[5m]))",
                        "legendFormat": "50th percentile"
                    }
                ],
                "yAxes": [
                    {
                        "unit": "s"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 8
                }
            },
            {
                "id": 5,
                "title": "Spatial Query Performance",
                "type": "graph",
                "targets": [
                    {
                        "expr": "histogram_quantile(0.95, rate(alyx_dataprocessing_spatial_query_time_bucket[5m]))",
                        "legendFormat": "Spatial Query 95th percentile"
                    }
                ],
                "yAxes": [
                    {
                        "unit": "s"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 12,
                    "y": 8
                }
            },
            {
                "id": 6,
                "title": "Cache Operations",
                "type": "graph",
                "targets": [
                    {
                        "expr": "rate(alyx_dataprocessing_cache_hits_total[5m])",
                        "legendFormat": "Cache Hits/sec"
                    },
                    {
                        "expr": "rate(alyx_dataprocessing_cache_misses_total[5m])",
                        "legendFormat": "Cache Misses/sec"
                    }
                ],
                "gridPos": {
                    "h": 8,
                    "w": 12,
                    "x": 0,
                    "y": 16
                }
            },
            {
                "id": 7,
                "title": "Slow Queries",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(alyx_dataprocessing_queries_slow_total[5m])",
                        "legendFormat": "Slow Queries/sec"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": 0
                                },
                                {
                                    "color": "yellow",
                                    "value": 0.1
                                },
                                {
                                    "color": "red",
                                    "value": 1
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 12,
                    "y": 16
                }
            },
            {
                "id": 8,
                "title": "Connection Leaks",
                "type": "stat",
                "targets": [
                    {
                        "expr": "rate(alyx_dataprocessing_connections_leaked_total[5m])",
                        "legendFormat": "Connection Leaks/sec"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "thresholds"
                        },
                        "thresholds": {
                            "steps": [
                                {
                                    "color": "green",
                                    "value": 0
                                },
                                {
                                    "color": "red",
                                    "value": 0.01
                                }
                            ]
                        }
                    }
                },
                "gridPos": {
                    "h": 8,
                    "w": 6,
                    "x": 18,
                    "y": 16
                }
            }
        ],
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "refresh": "5s"
    }
}